{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {}
    },
    {
      "src": "server/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/server/api.ts"
    },
    {
      "source": "/webhooks",
      "destination": "/server/webhooks.ts"
    }
  ]
}
